{
    "name": "m-adamski/symfony-directory-bundle",
    "description": "Directory tool created for faster directory management",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Maciej Adamski",
            "email": "m.adamski@ideacode.pl"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-fileinfo": "*",
        "symfony/framework-bundle": "^3.4|^4.1"
    },
    "require-dev": {
        "kint-php/kint": "^3.0",
        "phpunit/phpunit": "^7.4",
        "symfony/phpunit-bridge": "^4.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Adamski\\Symfony\\DirectoryBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adamski\\Symfony\\DirectoryBundleTests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
